    "test": "jest"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "devDependencies": {
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^3.3.0",
    "eslint": "^8.7.0",
    "eslint-config-preact": "^1.3.0",
    "jest": "^27.4.7",
    "jest-preset-preact": "^4.0.5",
    "preact-cli": "^3.3.3",
    "sirv-cli": "2.0.2"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.0",
    "@types/react": "^17.0.38",
    "autoprefixer": "^10.4.2",
    "graphql": "^16.2.0",
    "markdown-to-jsx": "^7.1.6",
    "netlify-cms-app": "^2.15.63",
    "netlify-identity-widget": "^1.9.2",
    "postcss": "^8.4.5",
    "preact": "^10.6.4",
    "preact-render-to-string": "^5.1.19",
    "preact-router": "^4.0.0",
    "react": "npm:preact@^10.6.4",
    "react-dom": "npm:preact@^10.6.4",
    "tailwindcss": "^3.0.15",
    "typescript": "^4.5.4"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
